{
  "name": "svgtiler",
  "description": "Tool for drawing diagrams on a grid, combining grids of SVGs into a big SVG figure",
  "version": "2.4.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edemaine/svgtiler.git"
  },
  "bugs": {
    "url": "https://github.com/edemaine/svgtiler/issues"
  },
  "homepage": "https://github.com/edemaine/svgtiler",
  "author": {
    "name": "Erik Demaine",
    "email": "edemaine@mit.edu",
    "url": "http://erikdemaine.org/"
  },
  "license": "MIT",
  "directories": {
    "lib": "lib"
  },
  "main": "lib/svgtiler.js",
  "bin": {
    "svgtiler": "lib/svgtiler.js"
  },
  "preferGlobal": true,
  "scripts": {
    "prepare": "coffee --no-header --bare -o lib -c src/svgtiler.coffee",
    "test": "make test"
