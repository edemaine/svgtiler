{
  "name": "svgtiler",
  "description": "Tool for drawing diagrams on a grid, combining grids of SVGs into a big SVG figure",
  "version": "2.1.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edemaine/svgtiler.git"
  },
  "bugs": {
    "url": "https://github.com/edemaine/svgtiler/issues"
  },
  "homepage": "https://github.com/edemaine/svgtiler",
  "author": {
    "name": "Erik Demaine",
    "email": "edemaine@mit.edu",
    "url": "http://erikdemaine.org/"
  },
  "license": "MIT",
  "directories": {
    "lib": "lib"
  },
  "main": "lib/svgtiler.js",
  "bin": {
    "svgtiler": "lib/svgtiler.js"
  },
  "preferGlobal": true,
  "scripts": {
    "prepare": "coffee --no-header --bare -o lib -c src/svgtiler.coffee",
    "test": "make test"
  },
  "dependencies": {
    "@babel/core": "7.15.0",
    "@babel/plugin-transform-react-jsx": "7.14.9",
    "@xmldom/xmldom": "^0.7.2",
    "async-limiter": "2.0.0",
    "coffeescript": "2.5.1",
    "csv-parse": "4.16.0",
    "grapheme-splitter": "1.0.4",
    "image-size": "1.0.0",
    "preact": "10.5.14",
    "preact-render-to-string": "5.1.19",
    "prettify-xml": "1.2.0",
    "stylus": "0.54.8",
    "xlsx": "0.17.1"
  },
  "devDependencies": {
    "vnu-jar": "21.6.11"
  }
}
